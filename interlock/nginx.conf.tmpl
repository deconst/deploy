# managed by interlock
user nginx;
worker_processes 2;

events {
  worker_connections 1024;
}

http {
  access_log /var/log/nginx/access.log;
  error_log /var/log/nginx/error.log;

  upstream servers {
    {{ range $up := $host.Upstream.Servers }}server {{ $up.Addr }};
    {{ end }}
  }

  server {
    listen 80;

    location / {
      proxy_pass http://servers;
    }
  }
}
